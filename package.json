{
  "name": "cardmass",
  "version": "1.9.0",
  "private": true,
  "scripts": {
    "bump:patch": "npm version patch --no-git-tag-version && node scripts/sync-version.mjs",
    "bump:minor": "npm version minor --no-git-tag-version && node scripts/sync-version.mjs",
    "bump:major": "npm version major --no-git-tag-version && node scripts/sync-version.mjs",
    "predev": "npm run bump:patch",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "version:check": "node scripts/sync-version.mjs --check"
  },
  "dependencies": {
    "mongoose": "^8.18.1",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
